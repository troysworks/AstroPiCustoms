[Unit]
Description=AstroPiCustoms
After=network.target
StartLimitBurst=5
StartLimitIntervalSec=300

[Service]
Type=simple
Restart=always
RestartSec=10
WorkingDirectory=/home/pi/AstroPiCustoms
ExecStart=/home/pi/AstroPiCustoms/venv/bin/python /home/pi/AstroPiCustoms/astropicustoms.py

[Install]
WantedBy=multi-user.target